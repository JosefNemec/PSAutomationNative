            <TableColumnItems>
              <TableColumnItem>
                <ScriptBlock>$_.Current.Name</ScriptBlock>
              </TableColumnItem>
              <TableColumnItem>
                <ScriptBlock>$_.Current.LocalizedControlType</ScriptBlock>
              </TableColumnItem>
              <TableColumnItem>
                <ScriptBlock>$_.Current.AutomationId</ScriptBlock>
              </TableColumnItem>
            </TableColumnItems>
          </TableRowEntry>
        </TableRowEntries>
        </TableControl>
    </View>
    <View>
      <Name>System.Windows.Automation.AutomationElement</Name>
