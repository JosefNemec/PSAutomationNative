            <ListItems>
                <ListItem>
                  <Label>AutomationID</Label>
                  <ScriptBlock>$_.Current.AutomationId</ScriptBlock>
                </ListItem>
                <ListItem>
                  <Label>ClassName</Label>
                  <ScriptBlock>$_.Current.ClassName</ScriptBlock>
                </ListItem>
                <ListItem>
                  <Label>ItemType</Label>
                  <ScriptBlock>$_.Current.ItemType</ScriptBlock>
                </ListItem>
                <ListItem>
                  <Label>IsEnabled</Label>
                  <ScriptBlock>$_.Current.IsEnabled</ScriptBlock>
                </ListItem>
                <ListItem>
                  <Label>LocalizedControlType</Label>
                  <ScriptBlock>$_.Current.LocalizedControlType</ScriptBlock>
                </ListItem>
                <ListItem>
                  <Label>Name</Label>
                  <ScriptBlock>$_.Current.Name</ScriptBlock>
                </ListItem>
                <ListItem>
                  <Label>NativeWindowHandle</Label>
                  <ScriptBlock>$_.Current.NativeWindowHandle</ScriptBlock>
                </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>
  </ViewDefinitions>
</Configuration>